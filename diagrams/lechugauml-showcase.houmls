(defn draw-triangle [p1x p1y p2x p2y p3x p3y]
   (def turtle-filler (new :org.homs.lechugauml.Turtle$PolylineFiller))
   (turtle-filler :add p1x p1y)
   (turtle-filler :add p2x p2y)
   (turtle-filler :add p3x p3y)
   (turtle-filler :fill *graphics*))

(defn sierpinski [n p1x p1y p2x p2y p3x p3y]
   (if (= n 0)
      (draw-triangle p1x p1y p2x p2y p3x p3y)
      (let {
         [p12x (/ (+ p1x p2x) 2)]
         [p12y (/ (+ p1y p2y) 2)]
         [p23x (/ (+ p2x p3x) 2)]
         [p23y (/ (+ p2y p3y) 2)]
         [p31x (/ (+ p3x p1x) 2)]
         [p31y (/ (+ p3y p1y) 2)] }
         
         (sierpinski (- n 1) p1x p1y p12x p12y p31x p31y)
         (sierpinski (- n 1) p12x p12y p2x p2y p23x p23y)
         (sierpinski (- n 1) p31x p31y p23x p23y p3x p3y))))


