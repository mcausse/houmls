(def pi (field-static :java.lang.Math :PI))
(def pi2 (* 2.0 pi))
(def pi2-div-3 (/ pi2 3))
(defn math/sin [angle] (call-static :java.lang.Math :sin angle))

(defn get-y [x cycles offset-rads]
   (let 
      {
         [total-rads (* cycles 2.0 pi)]
         [angle (+ offset-rads (/ (* total-rads x) box-width))]
         [center-y (+ box-y (/ box-height 2.0))]
         [y (+ center-y (* (/ box-height 2.0) (math/sin angle)))]
      }
      (to-int y)))

(def cycles 3)

(*this* :setColor *graphics* :RED)
(def x 0)
(while (&lt; x box-width)
   (def y (get-y x cycles 0.0))
   (*this* :drawRect *graphics* (+ box-x x) y 1 1)
   (set x (+ x 1)))

(*this* :setColor *graphics* :GREEN)
(def x 0)
(while (&lt; x box-width)
   (def y (get-y x cycles pi2-div-3))
   (*this* :drawRect *graphics* (+ box-x x) y 1 1)
   (set x (+ x 1)))

(*this* :setColor *graphics* :BLUE)
(def x 0)
(while (&lt; x box-width)
   (def y (get-y x cycles (* 2 pi2-div-3)))
   (*this* :drawRect *graphics* (+ box-x x) y 1 1)
   (set x (+ x 1)))</panel_attributes>
